<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
  <managed-bean>
        <managed-bean-name>bLogin</managed-bean-name>
        <managed-bean-class>beans.bLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
        <managed-bean-name>bAdministrador</managed-bean-name>
        <managed-bean-class>beans.bAdministrador</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tecnico</from-outcome>
            <to-view-id>/tecnico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>administrador</from-outcome>
            <to-view-id>/administrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/usuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administradorGestionTecnico.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tecnicoAltaCorrecta</from-outcome>
            <to-view-id>/administradorGestionTecnico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tecnicoAltaIncorrecta</from-outcome>
            <to-view-id>/administradorGestionTecnico.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administradorModificarPedido.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/administradorModificarPedido.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administrador.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>telefonoAltaIncorrecta</from-outcome>
            <to-view-id>/administrador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>telefonoAltaCorrecta</from-outcome>
            <to-view-id>/administrador.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>

