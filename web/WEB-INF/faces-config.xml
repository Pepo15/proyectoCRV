<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
  <managed-bean>
        <managed-bean-name>bLogin</managed-bean-name>
        <managed-bean-class>beans.bLogin</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
    <navigation-rule>
        <from-view-id>/administradorGestionTecnico.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/administradorGestionTecnico.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administradorModificarPedido.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/administradorModificarPedido.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>bAdministradorGestionTecnico</managed-bean-name>
        <managed-bean-class>beans.bAdministradorGestionTecnico</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bAdministradorGestionTelefono</managed-bean-name>
        <managed-bean-class>beans.bAdministradorGestionTelefono</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bAdministradorGestionPremio</managed-bean-name>
        <managed-bean-class>beans.bAdministradorGestionPremio</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bAdministradorModificarPedido</managed-bean-name>
        <managed-bean-class>beans.bAdministradorModificarPedido</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bAdministradorModificarReparacion</managed-bean-name>
        <managed-bean-class>beans.bAdministradorModificarReparacion</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bTecnico</managed-bean-name>
        <managed-bean-class>beans.bTecnico</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bUsuarioGestionPersonal</managed-bean-name>
        <managed-bean-class>beans.bUsuarioGestionPersonal</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bUsuarioGestionPedido</managed-bean-name>
        <managed-bean-class>beans.bUsuarioGestionPedido</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>bComprar</managed-bean-name>
        <managed-bean-class>beans.bComprar</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/administradorGestionTelefono.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/administradorGestionTelefono.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administradorGestionPremio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/administradorGestionPremio.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/administradorModificarReparaciones.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/administradorModificarReparaciones.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/tecnico.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/tecnico.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/usuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/usuario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/comprarTelefono.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>correcto</from-outcome>
            <to-view-id>/comprarTelefono.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/cesta.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>finalizar</from-outcome>
            <to-view-id>/finalizarPedido.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>tecnico</from-outcome>
            <to-view-id>/tecnico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>pUsuario</from-outcome>
            <to-view-id>/usuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>administrador</from-outcome>
            <to-view-id>/administrador.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/comprar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>verTelefono</from-outcome>
            <to-view-id>/comprarTelefono.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>

